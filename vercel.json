{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/login",
      "dest": "index.js"
    },
    {
      "src": "/callback",
      "dest": "index.js"
    },
    {
      "src": "/train-model",
      "dest": "index.js"
    },
    {
      "src": "/create-playlist",
      "dest": "index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
